---
layout: blank
---
<?xml version="1.0" encoding="UTF-8" ?>
<articles xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for post in site.posts %}
<article>
	<title>{{ post.title }}</title>
	<url>{{ site.url }}{{ post.url }}</url>
	<date>{{ post.date | date_to_utc | date: '%Y-%m-%d' }}</date>
</article>
{% endfor %}
</articles>